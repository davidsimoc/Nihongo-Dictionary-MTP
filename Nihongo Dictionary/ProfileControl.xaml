<UserControl x:Class="Nihongo_Dictionary.ProfileControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Nihongo_Dictionary"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid Background="{StaticResource BackgroundColor}">
        <TextBlock Text="Profil" FontSize="36" Foreground="{StaticResource TextColor}" HorizontalAlignment="Center" VerticalAlignment="Center" />
    </Grid>
</UserControl>
